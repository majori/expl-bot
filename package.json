{
  "name": "expl-bot",
  "version": "0.1.2",
  "description": "expl-bot",
  "main": "build/index.js",
  "scripts": {
    "start": "node build/index.js",
    "build": "tsc --listEmittedFiles",
    "build:test": "tsc --noEmit",
    "dev": "nodemon --inspect -r dotenv/config -r ts-node/register src/index.ts",
    "mocha": "mocha --exit test/**/*.test.ts",
    "mocha:watch": "mocha -w --inspect test/**/*.test.ts",
    "lint": "tslint -p tsconfig.json",
    "migrate": "knex migrate:latest",
    "test": "npm run lint && npm run build:test && npm run mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/majori/expl-bot.git"
  },
  "author": "Antti Kivim√§ki <antti.kivimaki@futurice.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/majori/expl-bot/issues"
  },
  "homepage": "https://github.com/majori/expl-bot#readme",
  "dependencies": {
    "@types/knex": "^0.14.14",
    "@types/winston": "^2.3.9",
    "knex": "^0.14.6",
    "lodash": "^4.17.10",
    "pg": "^7.4.3",
    "string-template": "^1.0.0",
    "telegraf": "^3.22.1",
    "ts-node": "^6.0.3",
    "typescript": "^3.0.1",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/lodash": "^4.14.109",
    "@types/mocha": "^5.2.0",
    "@types/node": "^10.1.2",
    "@types/sinon": "^5.0.1",
    "@types/string-template": "^1.0.2",
    "chai": "^4.1.2",
    "dotenv": "^5.0.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.4",
    "sinon": "^6.1.4",
    "tslint": "^5.10.0"
  }
}
